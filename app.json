    "android": {
      "package": "com.dickbraam.sudokudream",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dabce465-7915-4ff0-ab19-8fdfbe45fed6"
      }
    }
  }
