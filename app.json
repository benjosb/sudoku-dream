{
  "expo": {
    "name": "Sudoku Dream",
    "slug": "sudoku-dream",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dickbraam.sudokudream",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "We need camera access to scan Sudoku puzzles from newspapers and books.",
        "NSPhotoLibraryUsageDescription": "We need photo library access to load Sudoku puzzles from your photos."
      }
    },
    "android": {
      "package": "com.dickbraam.sudokudream",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dabce465-7915-4ff0-ab19-8fdfbe45fed6"
      }
    }
  }
}
